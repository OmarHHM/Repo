<fieldset name="wizardFields">
	<div class="col-sm-12">
		<div class="col-sm-3">
			<section class="form-group">
				<label class="control-label">Pais</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-pencil"></i></span>
					<input id="pais"  style="text-transform:uppercase;" name="pais" class="form-control"  text-style="upper" type="text" maxlength="50" >
				</div>
			</section>
		</div>
		<div class="col-sm-9">
			<section class="form-group">
				<label class="control-label">Calle</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-pencil"></i></span>
					<input id="calle" name="calle" style="text-transform:uppercase;" class="form-control"  text-style="upper" type="text" maxlength="100" >
				</div>
			</section>
		</div>
	</div>
	<div class="col-sm-12">
		<div class="col-sm-3">
			<section class="form-group">
				<label class="control-label">No. exterior</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-slack"></i></span>
					<input id="numExt" name="numExt" class="form-control" type="text" maxlength="10" >
				</div>
			</section>
		</div>
		<div class="col-sm-3">
			<section class="form-group">
				<label class="control-label">No. interior</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-slack"></i></span>
					<input id="numInt" name="numInt" class="form-control" type="text" maxlength="10" >
				</div>
			</section>
		</div>
		<div class="col-sm-3">
			<section class="form-group">
				<label class="control-label">Estado</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-pencil"></i></span>
					<input id="estado" name="estado"  style="text-transform:uppercase;" text-style="upper" class="form-control" type="text" maxlength="30" >
				</div>
			</section>
		</div>
		<div class="col-sm-3">
			<section class="form-group">
				<label class="control-label">Delegaci&oacute;n</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-pencil"></i></span>
					<input id="delegacion" name="delegacion"  style="text-transform:uppercase;" text-style="upper" class="form-control" type="text" maxlength="50" >
				</div>
			</section>
		</div>
	</div>
	
	<div class="col-sm-12">
		<div class="col-sm-3">
			<section class="form-group">
				<label class="control-label">Colonia</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-pencil"></i></span>
					<input id="colonia" name="colonia" style="text-transform:uppercase;" text-style="upper" class="form-control" type="text" maxlength="50" >
				</div>
			</section>
		</div>
		<div class="col-sm-3">
			<section class="form-group">
				<label class="control-label">C.P</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-pencil"></i></span>
					<input id="codigoPostal" name="codigoPostal" class="form-control" maxlength="5" >
				</div>
			</section>
		</div>
		<div class="col-sm-6">
			<section class="form-group">
				<label class="control-label">Referencia</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="icon-append fa fa-info-circle"></i></span>
					<input id="referencia" name="referencia"  style="text-transform:uppercase;" class="form-control" type="text" maxlength="200" >
				</div>
			</section>
		</div>
	</div>
</fieldset>